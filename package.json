{
  "name": "slugly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.18.2",
    "node-sass": "^4.11.0",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.0",
    "react-scripts": "^2.1.3",
    "typescript": "3.2.2"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-server": "tsc -w -p server",
    "nodemon-server": "nodemon dist/server/app.js",
    "server-dev": "concurrently \"npm run watch-server\" \"npm run nodemon-server\"",
    "postbuild": "rm -rf dist/ && tsc -p server && mv build dist/ && cp package.json dist/ && cp .npmrc dist && cd dist && zip -r ../dist.zip .[^.]*",
    "app": "node server/app.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
